<% if model.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-warning">
      <%= I18n.t('error.count', count: model.errors.count) %>
      <ul>
        <% model.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>

  </div>
<% end %>
